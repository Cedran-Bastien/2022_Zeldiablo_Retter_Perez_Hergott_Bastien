@startuml
'https://plantuml.com/sequence-diagram

main -> labyJeu: update(seconde, clavier)
activate labyJeu

labyJeu-> perso : getX()
activate perso
perso --> labyJeu : 1
deactivate perso

labyJeu -> perso : getY()
activate perso
perso --> labyJeu : 1
deactivate perso

labyJeu -> laby : getCase(1,1)
activate laby
laby --> labyJeu :CasePieges cp
deactivate laby

labyJeu -> cp : getType()
activate cp
cp --> labyJeu : 'p'
deactivate cp

labyJeu -> cp : faireEffet(perso)
activate cp
cp -> perso : perdrePv(cp.degat)
activate perso
perso --> cp
deactivate perso
cp --> labyJeu
deactivate cp
labyJeu --> main
deactivate labyJeu

@enduml